command /speed [<number>] [<player>]:	
	description: Receive the Skull of a Player
	permission: skess.speed
	usage: /speed <speed> [player] 
	permission message: {@noperm}
	trigger:
		if arg 1 is not set:
			if the player is flying:
				send "{@prefix}Your current flying speed is %the player's flying speed%."
				send "{@prefix}You must specify a speed to change to."
				stop
			else:
				send "{@prefix}Your current walking speed is %the player's walking speed%."
				send "{@prefix}You must specify a speed to change to."
				stop
		if arg 1 is set:
			if arg 2 is not set:
				if the player is flying:
					set the player's fly speed to arg 1
					send "{@prefix}Your flying speed is now %the player's flying speed%."
					stop
				else:
					set the player's walk speed to arg 1
					send "{@prefix}Your walking speed is now %the player's walking speed%."
					stop
			if arg 2 is set:
				if player has permission "skess.speed.others":
					if arg 2 is flying:
						set arg 2's fly speed to arg 1
						send "{@prefix}%arg 2%'s flying speed is now %arg 2's flying speed%."
						stop
					else:
						set arg 2's walk speed to arg 1
						send "{@prefix}%arg 2%'s walking speed is now %arg 2's walking speed%."
						stop
				else:
					send "{@noperm}"
					stop
